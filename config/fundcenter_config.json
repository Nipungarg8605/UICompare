  "expected_differences": {
    "missing_in_modern": [
      "ETF Basket Rates navigation",
      "ETF Valuation navigation", 
      "Baskets Summary Information",
      "Valuations Summary Information",
      "Production Build Date in footer"
    ],
    "format_differences": {
      "date_format": {
        "legacy": "MM/DD/YYYY",
        "modern": "DD/MM/YYYY"
      },
      "brand_tagline": {
        "legacy": "New ways. New answers.",
        "modern": "Global Fund Services"
      }
    },
    "data_differences": {
      "rates_total_count": {
        "legacy": "2482",
        "modern": "0",
        "note": "Legacy shows actual data, modern shows placeholder"
      }
    }
  },
  
  "test_scenarios": [
    {
      "name": "Navigation Structure Validation",
      "description": "Verify all navigation menu items are present and functional",
      "priority": "high"
    },
    {
      "name": "Dashboard Content Comparison", 
      "description": "Compare rates summary tables and warning messages",
      "priority": "high"
    },
    {
      "name": "Search Functionality Test",
      "description": "Test date picker and search button behavior",
      "priority": "high"
    },
    {
      "name": "Data Accuracy Verification",
      "description": "Verify counts and statuses match between systems",
      "priority": "medium"
    },
    {
      "name": "Missing Features Documentation",
      "description": "Document features present in legacy but not in modern",
      "priority": "medium"
    }
  ],
  
  "authentication": {
    "required": true,
    "type": "form_based",
    "note": "Both applications require login credentials"
  },
  
  "performance_expectations": {
    "modern_should_be_faster": true,
    "modern_should_be_responsive": true,
    "legacy_should_have_server_side_rendering": true
  }
}
